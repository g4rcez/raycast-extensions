{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dev-toolbelt",
  "owner": "g4rcez",
  "title": "Dev Toolbelt",
  "description": "Set of great tools to improve your developer experience",
  "icon": "command-icon.png",
  "access": "public",
  "author": "g4rcez",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "keywords": ["Toolbelt", "Dev Utils"],
  "commands": [
    {
      "name": "index",
      "title": "Dev Toolbelt",
      "subtitle": "Open toolbelt tools",
      "description": "Open your toolbelt tools",
      "mode": "view"
    },
    {
      "name": "decodeBase64",
      "subtitle": "Dev Toolbelt",
      "title": "Decode Base64",
      "description": "Decode Base64",
      "mode": "view"
    },
    {
      "name": "encodeBase64",
      "subtitle": "Dev Toolbelt",
      "title": "Encode Text to Base64",
      "description": "Encode text to base64",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.61.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}